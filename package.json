{
  "name": "web-enc-at-rest",
  "version": "0.1.0",
  "description": "Encryption-at-Rest for Web Apps Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf coverage dist",
    "clean:modules": "rm -rf node_modules package-lock.json && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom=\"./src/**\"",
    "lint": "eslint . --ext .js,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikh2000/web-enc-at-rest.git"
  },
  "keywords": [
    "encryption-at-rest",
    "password-based",
    "password-based cryptography",
    "password-based encryption",
    "pbe",
    "pkcs5",
    "pkcs#5",
    "offline-first"
  ],
  "author": "Erik Hermansen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikh2000/web-enc-at-rest/issues"
  },
  "homepage": "https://github.com/erikh2000/web-enc-at-rest#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">= 16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 100
      }
    }
  }
}
